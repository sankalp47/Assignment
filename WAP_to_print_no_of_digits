
import java.util.*;
import java.io.*;

public class Main {
    public static void main(String args[]) {
	      Scanner sc = new Scanner(System.in);
	      int n = sc.nextInt();
	      int m = 0;
	      while(n>0){
	          n=n/10;
	          m = m+1;
	          System.out.println(m);
	      }
    }    
   
}



